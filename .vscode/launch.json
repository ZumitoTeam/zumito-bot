{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Bot",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "runtimeArgs": ["--import", "tsx", "--preserve-symlinks"],
  
        "args": ["node_modules/.bin/zumito-framework"],
        
        "cwd": "${workspaceRoot}",
        "internalConsoleOptions": "openOnSessionStart",
        "skipFiles": ["<node_internals>/**", "node_modules/**"],
        
      }
    ]
}